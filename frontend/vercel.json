{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://tehzeeb.onrender.com/api/$1"
    }
  ],
  "redirects": [],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
